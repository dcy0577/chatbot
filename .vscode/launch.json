{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "fastapi",
            "type": "python",
            "request": "launch",
            "program": "main.py",
            "cwd": "${workspaceFolder}/backend/app",
            "justMyCode": false,
        },
        {
            "name": "streamlit",
            "type": "python",
            "module": "streamlit",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "args": [
                "run",
                "streamlit/main.py",
                "--server.port",
                "9999",
                "--logger.level=debug",
           ]
        },
    ]
}